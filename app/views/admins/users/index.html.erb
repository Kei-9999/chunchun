<div class="font">
  <div class="item-visual">
 　<div class="container">

   <h4>▼会員一覧</h4>
     <div>
      <div class="col-12">
       <table class="table table-hover table-striped">
        <thead>
          <tr>
            <th>ID</th>
            <th>氏名</th>
            <th>メールアドレス</th>
            <th></th>
          
          </tr>
        </thead>

        <tbody>
          <% @users.each do |user| %>
            <tr>
              <td><%= user.id %></td>
              <td><%= link_to admins_user_path(user) do %>
                  <%= user.name %>
                  <% end %>
　            </td>
              <td><%= user.email %></td>
              <td><%= link_to 'Delete', admins_user_path(user.id), method: :delete %></td>
              <!--idを入れる-->
            </tr>
          <% end %>
        </tbody>
      </table>
      <%= paginate @users %>
    </div>
   </div>
  </div>
 </div>